<div class="container m-1 p-1 rcorners miding listcontainer">
          <p class="divtitle">Ingredient</p>
          <p>
            Role:
            <b>
              {{ miding.role.coding[0].display }}
            </b>
          </p>

          <p>
            Substance:
            <b>
              {{ miding.substance.code.concept.coding[0].display }}
            </b>
          </p>

          {% for strn in miding.substance.strength %}
            {% if strn.concentrationRatio %}
              <p>
                Concentration strength:
                <b>
                  {{ strn.concentrationRatio.numerator.value }}{{ strn.concentrationRatio.numerator.unit }} /
                  {{ strn.concentrationRatio.denominator.value }}{{ strn.concentrationRatio.denominator.unit }}
                </b>
              </p>
            {% endif %}
          {% endfor %}

          {% for strn in miding.substance.strength %}
            {% if strn.presentationRatio %}
              <p>
                Presentation strength:
                <b>
                  {{ strn.presentationRatio.numerator.value }} {{ strn.presentationRatio.numerator.unit }} /

                  {{ strn.presentationRatio.denominator.value }} {{ strn.presentationRatio.denominator.unit }}
                </b>
              </p>
            {% endif %}
          {% endfor %}
          {% for strn in miding.substance.strength %}
            {% if strn.referenceStrength %}
            <hr>
              <p>
                Reference strength:</br>
                Substance:
                <b>
                  {{ strn.referenceStrength.substance.concept.coding[0].display }}
                </b>
                <b>

                  {{ strn.referenceStrength.strengthRatio.numerator.value }} {{ strn.referenceStrength.strenthRatio.numerator.unit }} /

                  {{ strn.referenceStrength.strengthRatio.denominator.value }} {{ strn.referenceStrength.strengthRatio.denominator.unit }}
                </b>
              </p>
            {% endif %}
          {% endfor %}
        </div>
        {% comment %}{% endcomment %}