<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="Patient" />
  <url value="http://hl7.eu/fhir/ig/unicom/StructureDefinition/Patient" />
  <name value="Patient" />
  <title value="Patient (Logical)" />
  <status value="draft" />
  <description value="Description of the Patient Model" />
  <kind value="logical" />
  <abstract value="false" />
  <type value="Patient" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Element" />
  <derivation value="specialization" />
  <differential>
    <element id="Patient">
      <path value="Patient" />
      <short value="Patient" />
    </element>
    <element id="Patient.identifier">
      <path value="Patient.identifier" />
      <short value="Regional/National Health Id" />
      <definition value="If the patient has a regional or national Health Identification" />
      <min value="1" />
      <max value="1" />
      <type>
        <code value="http://hl7.eu/fhir/ig/unicom/StructureDefinition/Identifier" />
      </type>
    </element>
    <element id="Patient.socialNumber">
      <path value="Patient.socialNumber" />
      <short value="Social/Insurance Number" />
      <definition value="If a patient has both, national/regional ID and Social/Insurance number, only the regional/national Health Id is required by law.&#xA;If the only identification the patient has is the Social/insurance number, then this one is considered as the regional/national Health Id. This field is required by some national laws." />
      <min value="0" />
      <max value="1" />
      <type>
        <code value="http://hl7.eu/fhir/ig/unicom/StructureDefinition/Identifier" />
      </type>
    </element>
    <element id="Patient.name">
      <path value="Patient.name" />
      <short value="Patient Name" />
      <definition value="The surname/s of the patient" />
      <min value="1" />
      <max value="*" />
      <type>
        <code value="http://hl7.eu/fhir/ig/unicom/StructureDefinition/PersonName" />
      </type>
    </element>
    <element id="Patient.birthDate">
      <path value="Patient.birthDate" />
      <short value="Birth date" />
      <definition value="Date of birth&#xA;01/01/2009&#xA;This field may contain only the year" />
      <min value="1" />
      <max value="1" />
      <type>
        <code value="dateTime" />
      </type>
    </element>
    <element id="Patient.gender">
      <path value="Patient.gender" />
      <short value="Gender" />
      <definition value="The gender of the patient&#xA;This field can be empty&#xA;Example: Male/female/unknown" />
      <min value="0" />
      <max value="1" />
      <type>
        <code value="http://hl7.eu/fhir/ig/unicom/StructureDefinition/Codeable" />
      </type>
    </element>
  </differential>
</StructureDefinition>