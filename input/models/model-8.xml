      <max value="*" />
      <type>
        <code value="BackboneElement" />
      </type>
    </element>
    <element id="AbstractMedicinalProduct.doseForm.administrableDoseForm">
      <path value="AbstractMedicinalProduct.doseForm.administrableDoseForm" />
